@model CreateUPNvalueDto
@{
    ViewData["Title"] = "CreateUPNvalue";
    Layout = "~/Views/Admin/Index.cshtml";
}
<div class="container-fluid pt-4 px-4">
    <div class="row g-4">
        <div class="col-12">
            <div class="bg-light rounded h-100 p-4">
                <h6 class="mb-4">Yeni Ürün Parametre value Ekleme Sayfası</h6>
                <form method="post">
                    <label>UpatamaKodu</label>
                    <input type="text" asp-for="UpatamaKodu" class="form-control" />
                    <br />
                    <label>NumuneId</label>
                    <input type="member" asp-for="NumuneId" class="form-control" />
                    <br />
                    <label>Value</label>
                    <input type="text" asp-for="Value" class="form-control" />
                    <br />
                    <label>EklenmeTarihi</label>
                    <input type="text" asp-for="EklenmeTarihi" class="form-control" />
                    <br />
                    <label>PersonelSicilNo</label>
                    <input type="text" asp-for="PersonelSicilNo" class="form-control" />
                    <br />
                    <button class="btn btn-primary">Ürün Parametre value Kaydet</button>
                </form>
            </div>
        </div>
    </div>
</div
