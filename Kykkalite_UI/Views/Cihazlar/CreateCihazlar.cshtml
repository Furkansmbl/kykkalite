@model CreateCihazlarDto
@{
    ViewData["Title"] = "CreateCihazlar";
    Layout = "~/Views/Admin/Index.cshtml";
}

<div class="container-fluid pt-4 px-4">
    <div class="row g-4">
        <div class="col-12">
            <div class="bg-light rounded h-100 p-4">
                <h6 class="mb-4">Yeni Cihaz Ekleme Sayfası</h6>
                <form method="post">
                    <label>CihazID</label>
                    <input type="text" asp-for="CihazID" class="form-control" />
                    <br />
                    <label>CihazKodu</label>
                    <input type="text" asp-for="CihazKodu" class="form-control" />
                    <br />
                    <label>KullanılanCihazEkipman</label>
                    <input type="text" asp-for="KullanılanCihazEkipman" class="form-control" />
                    <br />
                    <label>FabrikaID</label>
                    <input type="text" asp-for="FabrikaID" class="form-control" />
                    <br />
                    <label>PersonelSicilNo</label>
                    <input type="text" asp-for="PersonelSicilNo" class="form-control" />
                    <br />
                    <label>CihazID</label>
                    <input type="text" asp-for="CihazID" class="form-control" />
                    <br />
                    <label>EklenmeGuncellenmeTarihi</label>
                    <input type="text" asp-for="EklenmeGuncellenmeTarihi" class="form-control" />
                    <br />
                    <label>KullanımDurumu</label>
                    <input type="text" asp-for="KullanımDurumu" class="form-control" />
                    <br />
                    <button class="btn btn-primary">Cihazı Kaydet</button>
                </form>
            </div>
        </div>
    </div>
</div>



