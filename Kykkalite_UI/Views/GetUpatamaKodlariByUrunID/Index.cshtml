            if (hatAdiAciklamasi) {
                document.getElementById("UretimHatlari").value = hatAdiAciklamasi;
            }

            document.getElementById("PersonelSicilNo").value = localStorage.getItem("PersonelSicilNo") || "@ViewBag.PersonelSicilNo";

            filterUretimHatlari(); 
        };
    </script>
</body>
</html>