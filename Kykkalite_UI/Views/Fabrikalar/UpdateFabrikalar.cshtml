@model UpdateFabrikalarDto
@{
    ViewData["Title"] = "UpdateFabrikalar";
    Layout = "~/Views/Admin/Index.cshtml";
}
<div class="container-fluid pt-4 px-4">
    <div class="row g-4">
        <div class="col-12">
            <div class="bg-light rounded h-100 p-4">
                <h6 class="mb-4">Fabrika Güncelleme Sayfası</h6>
                <form method="post">
                    <label>FabrikaID</label>
                    <input type="member" asp-for="FabrikaID" class="form-control" />
                    <br />
                    <label>Fabrika Adı</label>
                    <input type="text" asp-for="FabrikaAdi" class="form-control" />
                    <br />
                    <button class="btn btn-success">Fabrikayı Güncelle</button>
                </form>
            </div>
        </div>
    </div>
</div>
